@model FrondEnd_ProyectoPA.Models.Ruta

@{
    ViewData["Title"] = "EliminarRuta";
}
<div class="w3-container w3-blue  w3-text-black">

    <h4>Mantenimiento Ruta</h4>
    <hr />
    <h1>Eliminar Ruta</h1>
</div>
<div class="w3-container w3-light-grey w3-text-black">
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Id_Ruta)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Id_Ruta)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Nombre)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Nombre)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.MontoEstimado)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.MontoEstimado)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.MontoRecibido)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.MontoRecibido)
        </dd>
    </dl>

    <form asp-action="EliminarRuta">
        <input type="submit" value="Eliminar" class="btn btn-danger w3-input" />
        <a class="w3-button w3-green btn btn-secondary" asp-action="Index">Regresar</a>
    </form>
</div>
